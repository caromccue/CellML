    df["RatioClearTot"] = df["Num clear"] / df["Num drops"]
    # Compute the ratio of clear drops to total drops corrected for any artifact 'non-crystallized' drops present at the end
    df["CorrRatioClearTot"] = (df["Num clear"] - df["Num clear"].iloc[-1]) / (df["Num drops"] - df["Num clear"].iloc[-1])
    
