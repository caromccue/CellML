# Select columns containing the control / GQD results
control_col = [col for col in df if col.startswith('State_Ctrl')]
np_col = [col for col in df if col.startswith('State_NP')]

# Count the occurences of the diff states (NANs occur when the value_counts is 0)
df['Control_Num_Crystal'] = df[control_col].apply(pd.Series.value_counts, axis=1)['CRYSTAL']
df['Control_Num_Crystal'].fillna(value = 0, inplace = True)
df['Control_Num_Clear'] = df[control_col].apply(pd.Series.value_counts, axis=1)['CLEAR']
df['Control_Num_Clear'].fillna(value = 0, inplace = True)
df['Control_Num_Failed'] = df[control_col].apply(pd.Series.value_counts, axis=1)['FAILED']
df['Control_Num_Failed'].fillna(value = 0, inplace = True)
df['NP_Num_Crystal'] = df[np_col].apply(pd.Series.value_counts, axis=1)['CRYSTAL'] 
df['NP_Num_Crystal'].fillna(value = 0, inplace = True)
df['NP_Num_Clear'] = df[np_col].apply(pd.Series.value_counts, axis=1)['CLEAR'] 
df['NP_Num_Clear'].fillna(value = 0, inplace = True)
df['NP_Num_Failed'] = df[np_col].apply(pd.Series.value_counts, axis=1)['FAILED'] 
df['NP_Num_Failed'].fillna(value = 0, inplace = True)

# Evaluate the probability of a drop displaying crystals in a set of conditions by dividing by the number of valid drops
df['Control_Prob_Crystal'] = df['Control_Num_Crystal'] / (df['Control_Num_Crystal'] + df['Control_Num_Clear'])
df['NP_Prob_Crystal'] = df['NP_Num_Crystal'] / (df['NP_Num_Crystal'] + df['NP_Num_Clear'])

# Select only the index and newly created columns for display
processed_columns = ['[Lys]', '[NaCl]', 'Date', 'Funct', 'Control_Prob_Crystal', 'NP_Prob_Crystal',  'Control_Num_Crystal', 'Control_Num_Clear',
