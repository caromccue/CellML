for filename in os.listdir():
    if filename.endswith('.jpg'):
        im = Image.open(filename)
        date = '20' + filename[:2] + '-' + filename[2:4] + '-' + filename[4:6] + ' ' +  filename[7:9] + ':' + filename[9:11] + ':' + filename[11:13]
        parsedate = datetime.strptime(date, "%Y-%m-%d %H:%M:%S")

        exif_dict = {}
        exif_dict["Exif"] = {}
        exif_dict["Exif"][piexif.ExifIFD.DateTimeOriginal] = parsedate.strftime("%Y:%m:%d %H:%M:%S")
        exif_dict["Exif"][piexif.ExifIFD.DateTimeDigitized] = parsedate.strftime("%Y:%m:%d %H:%M:%S")
        exif_bytes = piexif.dump(exif_dict)
        new_filename = filename.split('.')[0] + '_with_exif' + '.jpg'
        im.save(new_filename, "jpeg", exif=exif_bytes, quality=95, )
